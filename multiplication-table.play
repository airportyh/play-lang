def main() [
    body = getDocumentBody()
    table = createElement("table", { border: "1" })
    appendTo(body, table)
    i = 1
    while i <= 3 [
        row = createElement("tr")
        appendTo(table, row)
        j = 1
        while j <= 3 [
            product = (i * j) + ""
            cell = createElement("td")
            appendTo(row, cell)
            setText(cell, product)
            j = j + 1
        ]
        i = i + 1
    ]
]
