def main() [
    answer = twoSum([2,7,11,15], 9)
    print(answer)
]

def twoSum(nums, target) [
    visited = {}
    i = 0
    while i < length(nums) [
        num = nums[i]
        other = target - num
        if has(visited, other) [
            return [i, visited[other]]
        ]
        i = i + 1
    ]
    return null
]